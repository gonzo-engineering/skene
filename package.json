		"@typescript-eslint/parser": "^8.26.1",
		"eslint": "^9.35.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-svelte": "^3.3.2",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.28.2",
		"svelte-check": "^4.1.5",
		"tslib": "^2.8.1",
		"typescript": "^5.8.2",
		"vite": "^6.3.5"
