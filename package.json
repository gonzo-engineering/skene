		"@typescript-eslint/eslint-plugin": "^8.8.1",
		"@typescript-eslint/parser": "^8.8.1",
		"eslint": "^9.12.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^3.2.0",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.7",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.4",
		"tslib": "^2.7.0",
		"typescript": "^5.6.2",
		"vite": "^5.4.8"
	},
	"type": "module"
