		"@typescript-eslint/parser": "^8.48.0",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.0",
		"prettier": "^3.7.3",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.45.3",
		"svelte-check": "^4.3.4",
		"tslib": "^2.8.1",
